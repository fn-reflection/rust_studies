[mysqld]
binlog_format=ROW
character-set-server=utf8mb4
datadir=/var/lib/mysql
default-time-zone='UTC'
innodb_file_format=Barracuda
skip-ssl=1
sql_mode=NO_ENGINE_SUBSTITUTION,STRICT_ALL_TABLES,ONLY_FULL_GROUP_BY
user=mysql

[mysqldump]
default-character-set=utf8mb4

[mysql]
default-character-set=utf8mb4